{
  "name": "@cambai/n8n-nodes-cambai",
  "version": "1.0.0",
  "description": "n8n integration for the CambAI API",
  "license": "MIT",
  "author": {
    "name": "CambAI Team",
    "email": "team@camb.ai"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cambai/n8n-nodes-cambai.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier --write nodes/**/*.ts credentials/**/*.ts",
    "lint": "eslint nodes credentials --ext .js,.ts",
    "lintfix": "eslint nodes credentials --ext .js,.ts --fix",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "credentials": [
      "dist/credentials/CambAIApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/CambAI/CambAi.node.js"
    ]
  },
  "keywords": [
    "n8n-community-node-package",
    "cambai",
    "ai dubbing",
    "text to speech",
    "voice translation"
  ],
  "devDependencies": {
    "@types/node": "^18.19.41",
    "@typescript-eslint/parser": "~5.62.0",
    "eslint": "~8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "n8n-workflow": "^1.82.0",
    "prettier": "^2.8.8",
    "typescript": "~4.9.5"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
